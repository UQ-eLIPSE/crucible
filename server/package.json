{
	"name": "chemistryhub-server",
	"version": "1.0.0",
	"description": "UQ Chemistry Resource Hub backend",
	"main": "./dist/index.js",
	"scripts": {
		"clean": "rimraf dist",
		"build": "yarn clean && tsc",
		"start": "yarn start:prod",
		"seed": "./get_database.sh",
		"start:prod": "cross-env NODE_ENV=production node ./dist/index.js",
		"start:dev": "cross-env NODE_ENV=development node ./dist/index.js",
		"watch": "nodemon",
		"test": "echo \"Error: no test specified\" && exit 1",
		"pretty": "prettier --write ./src",
		"lint": "eslint ./src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"convertQuiz": "ts-node src/util/QuizConversion.ts",
		"docker:convertQuiz": "docker exec -it crucible_server_1 sh -c 'yarn convertQuiz'"
	},
	"author": "eLIPSE",
	"license": "MIT",
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/body-parser": "1.19.2",
		"@types/bson": "^4.2.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.9",
		"@types/express": "^4.17.11",
		"@types/express-session": "^1.17.3",
		"@types/multer": "^1.4.5",
		"@types/node": "^18.11.18",
		"@types/passport": "^1.0.5",
		"@types/passport-local": "^1.0.33",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"@uq-elipse/uq-eait-sso": "^0.0.5",
		"cross-env": "7.0.3",
		"eslint": "^8.33.0",
		"prettier": "^2.8.3",
		"nodemon": "^2.0.21",
		"rimraf": "3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"bcryptjs": "2.4.3",
		"body-parser": "1.20.1",
		"connect-mongo": "^4.6.0",
		"cookie-parser": "1.4.6",
		"cors": "2.8.5",
		"express": "4.18.2",
		"express-session": "1.17.3",
		"manta": "^5.2.2",
		"mongodb": "^4.13.0",
		"multer": "1.4.5-lts.1",
		"node": "^19.3.0",
		"npm-check-updates": "^16.6.2",
		"passport": "0.4.1",
		"passport-local": "1.0.0",
		"taipu": "github:UQ-eLIPSE/taipu#v0.1.1",
		"ultima-launch": "github:UQ-eLIPSE/ultima-launch#v0.0.1",
		"uq-eait-sso": "github:UQ-eLIPSE/uq-eait-sso#v0.0.4"
	}
}
