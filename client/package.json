{
	"name": "client",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"build": "run-p type-check build-only",
		"preview": "vite preview",
		"test:jest": "jest ./tests/unit/* --config=jest.config.json --verbose",
		"test:e2e": "start-server-and-test preview :5173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 5173' :5173 'cypress open --e2e'",
		"test:unit": "cypress run --component",
		"test:unit:dev": "cypress open --component",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
		"pretty": "prettier --write ./src",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore", 
		"cypress:atdd": "cypress run --config specPattern=cypress/atdd --headed"
	},
	"dependencies": {
		"@types/d3": "^7.4.0",
		"axios": "^1.2.2",
		"d3": "^7.8.5",
		"form-data": "^4.0.0",
		"just-clone": "^6.2.0",
		"just-safe-set": "^4.2.1",
		"katex": "^0.16.4",
		"sass": "^1.57.1",
		"tinymce": "^6.3.1",
		"uuid": "^9.0.0",
		"uuidv4": "^6.2.13",
		"vue": "^3.2.45",
		"vue-router": "^4.1.6",
		"vuex": "^4.1.0"
	},
	"devDependencies": {
		"@types/d3": "^7.4.0",
		"@rushstack/eslint-patch": "^1.1.4",
		"@types/jest": "^29.5.0",
		"@types/katex": "^0.16.0",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"@vue/tsconfig": "^0.1.3",
		"cypress": "^12.0.2",
		"eslint": "^8.33.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-vue": "^9.3.0",
		"jest": "^29.4.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"start-server-and-test": "^1.15.2",
		"ts-jest": "^29.0.5",
		"typescript": "~4.7.4",
		"vite": "^4.0.0",
		"vue-tsc": "^1.0.12"
	}
}
